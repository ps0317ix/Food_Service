
<div class="main top-main">
  <div class="container">
    <div class="top-message">
      <p style="padding:10px 0px;">東京の主要駅を入力して検索</p>
      <%= form_tag("/search_exe",:method => 'get') do %>
        <%= text_field_tag :search %>
        <%= submit_tag 'Search', :name => nil %>
      <% end %>
    </div>
    <div id="map_info">
      <p id="venue">東京スカイツリー</p>
      <p id="address">東京都墨田区押上１−１−２</p>
      <p id="url"><a href="http://www.tokyo-skytree.jp/" target="_blank">www.tokyo-skytree.jp</a></p>
      <p id="zoom">17</p>
    </div>
    <div id="map_container">
      <div id="map_canvas"></div>
    </div>

  </div>




  <footer>
    <div class="container">
      <div class="footer_links">
        <h3>食べログ</h3>
        <ul>
        <%
          if @tabelogPlaces
            @tabelogPlaces.each do |place|
        %>
            <li><%= link_to "#{place.area}", "/place/#{place.area}" %></li>
        <%
            end
          end
        %>
        </ul>

    </div>
  </footer>



</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?language=ja&region=JP&key=AIzaSyCpM_NXjd2FB5C8spyddfKd9rDDVndEIkA&callback=initMap" async defer></script>
